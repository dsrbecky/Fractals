<item id="ref-24" xsi:type="SOAP-ENC:string">public static void GetColor(double p, double q,out double r,out double g,out double index)
{
	MakeColors();
	double bigX,bigY;
	double x=0,y=0;
	r = 0; g = 0; index = 64;
	for (int i = 0;i &#60;= 64;i++)
	{
		bigX = x*x - y*y + p;
		bigY = 2*x*y + q;
		if (double.IsNaN(bigX) || double.IsNaN(bigY))
		{
			index = i;
			return;
		}
		x = bigX;
		y = bigY;
	}
	return;
}</item>
